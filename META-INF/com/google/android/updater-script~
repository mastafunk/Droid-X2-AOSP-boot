ui_print("");
ui_print("");
ui_print("Checking device...");
assert(getprop("ro.product.device") == "daytona" || getprop("ro.build.product") == "daytona");
ui_print("Check done.  Continuing...");
ui_print("");
ui_print("");
ui_print("Droid X2 AOSP Boot Test");
ui_print("----==<<TESTING ONLY>>==----");
ui_print("==> Mounting System");
mount("ext3", "EMMC", "/dev/block/mmcblk0p12", "/system");
ui_print("");
ui_print("==> Extracting files...");
package_extract_dir("system", "/system");
run_program("/system/xbin/busybox", "mv", "/system/bin/mot_boot_mode", "/system/bin/mot_boot_mode.bin");
symlink("/system/bin/hijack2", "/system/bin/mot_boot_mode");
ui_print("");
ui_print("==> Setting permissions...");
set_perm_recursive(0, 0, 0755, 0644, "/system");
set_perm_recursive(0, 2000, 0755, 0755, "/system/bin");
set_perm(0, 2000, 04755, "/system/bin/2nd-init");
ui_print("");
ui_print("==> Finishing up...");
unmount("/system");
ui_print("");
ui_print("");
ui_print("Good Luck!!");
ui_print("");

